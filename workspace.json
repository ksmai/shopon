{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  },
  "defaultProject": "homepage",
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "projects": {
    "shared-assets": {
      "root": "libs/shared/assets",
      "sourceRoot": "libs/shared/assets",
      "projectType": "library",
      "targets": {}
    },
    "shared-styles": {
      "root": "libs/shared/styles",
      "sourceRoot": "libs/shared/styles/src",
      "projectType": "library",
      "targets": {}
    },
    "homepage": {
      "root": "apps/homepage",
      "sourceRoot": "apps/homepage",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "root": "apps/homepage",
            "outputPath": "dist/apps/homepage"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "homepage:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "homepage:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "homepage:build:production"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/homepage/**/*.{ts,tsx,js,jsx}"
            ]
          }
        }
      }
    },
    "homepage-e2e": {
      "root": "apps/homepage-e2e",
      "sourceRoot": "apps/homepage-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/homepage-e2e/cypress.json",
            "tsConfig": "apps/homepage-e2e/tsconfig.e2e.json",
            "devServerTarget": "homepage:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "homepage:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/homepage-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "homepage-custom-app": {
      "root": "libs/homepage/custom-app",
      "sourceRoot": "libs/homepage/custom-app/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/homepage/custom-app/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/homepage/custom-app"
          ],
          "options": {
            "jestConfig": "libs/homepage/custom-app/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "homepage-index-page": {
      "root": "libs/homepage/index-page",
      "sourceRoot": "libs/homepage/index-page/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/homepage/index-page/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/homepage/index-page"
          ],
          "options": {
            "jestConfig": "libs/homepage/index-page/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "homepage-default-layout": {
      "root": "libs/homepage/default-layout",
      "sourceRoot": "libs/homepage/default-layout/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/homepage/default-layout/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/homepage/default-layout"
          ],
          "options": {
            "jestConfig": "libs/homepage/default-layout/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "accounts": {
      "root": "apps/accounts",
      "sourceRoot": "apps/accounts/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/accounts",
            "main": "apps/accounts/src/main.ts",
            "tsConfig": "apps/accounts/tsconfig.app.json",
            "assets": []
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/accounts/src/environments/environment.ts",
                  "with": "apps/accounts/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "accounts:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/accounts/**/*.ts"
            ]
          }
        }
      }
    },
    "accounts-module": {
      "root": "libs/accounts/module",
      "sourceRoot": "libs/accounts/module/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/accounts/module/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/accounts/module"
          ],
          "options": {
            "jestConfig": "libs/accounts/module/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}